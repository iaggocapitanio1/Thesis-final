@article{GARRIDOJURADO20142280,
title = {Automatic generation and detection of highly reliable fiducial markers under occlusion},
journal = {Pattern Recognition},
volume = {47},
number = {6},
pages = {2280-2292},
year = {2014},
issn = {0031-3203},
doi = {https://doi.org/10.1016/j.patcog.2014.01.005},
