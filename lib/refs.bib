@inproceedings{pezoa,
author = {Pezoa, Felipe and Reutter, Juan L. and Suarez, Fernando and Ugarte, Mart\'{\i}n and Vrgo\v{c}, Domagoj},
title = {Foundations of JSON Schema},
year = {2016},
isbn = {9781450341431},
